{
  "name": "lens-backend",
  "version": "1.0.0",
  "description": "Backend API for lens. photography portfolio platform",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t lens-backend .",
    "docker:run": "docker run -p 5000:5000 --env-file .env lens-backend",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "setup": "./scripts/setup.sh",
    "setup:env": "node scripts/env-setup.js",
    "setup:secrets": "node scripts/env-setup.js --generate-secrets",
    "setup:full": "npm run setup && npm run setup:env"
  },
  "keywords": ["photography", "portfolio", "api", "mongodb"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@eslint/js": "^0.44.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
